{
  "version": 2,
  "projects": {
    "back-office-api": "apps/back-office/api",
    "back-office-web": "apps/back-office/web",
    "configuration-api": "apps/configuration/api",
    "configuration-web": "apps/configuration/web",
    "dashboard-api": "apps/dashboard/api",
    "dashboard-web": "apps/dashboard/web",
    "domain": "libs/domain",
    "gateway-api": "apps/gateway/api",
    "gateway-web": "apps/gateway/web",
    "model": "libs/model",
    "page": "libs/page",
    "shared": "libs/shared",
    "ui": "libs/ui"
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "gateway-web"
}
